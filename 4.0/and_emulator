
androidDir=`pwd`
imgDir="$androidDir/img/"
sdkDir="$androidDir/sdk/tools"
cmd="${sdkDir}/emulator -avd mydroid -system ${imgDir}system.img  -data ${imgDir}/userdata.img -ramdisk ${imgDir}/ramdisk.img  -kernel $imgDir/kernel-qemu-armv7 -sdcard /home/sin/wkspace/apk/sdcard.img -memory 512 -partition-size 512 "
$cmd


